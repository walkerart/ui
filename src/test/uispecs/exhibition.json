                                    },
                                    ".csc-preferredExhibition-exhibitionTermGroup": {
                                        "decorators": [
                                            {
                                                "func": "cspace.makeRepeatable",
                                                "type": "fluid",
                                                "options": {
                                                    "repeatTree": {
                                                        "expander": {
                                                            "tree": {
                                                                ".csc-exhibitionAuthority-termSourceID": "${{row}.termSourceID}",
                                                                ".csc-exhibitionAuthority-termName": "${{row}.termName}",
                                                                ".csc-exhibitionAuthority-termSourceDetail": "${{row}.termSourceDetail}",
                                                                ".csc-exhibitionAuthority-termPrefForLang": "${{row}.termPrefForLang}",
                                                                ".csc-exhibitionAuthority-termQualifier": "${{row}.termQualifier}",
                                                                ".csc-exhibitionAuthority-termType": {
                                                                    "optionnames": [
                                                                        "Please select a value",
                                                                        "Descriptor",
                                                                        "Alternate Descriptor",
                                                                        "Used for Term"
                                                                    ],
                                                                    "optionlist": [
                                                                        "",
                                                                        "descriptor",
                                                                        "alternate descriptor",
                                                                        "used for term"
                                                                    ],
                                                                    "selection": "${{row}.termType}"
                                                                },
                                                                ".csc-exhibitionAuthority-termSourceNote": "${{row}.termSourceNote}",
                                                                ".csc-exhibitionAuthority-termSource": "${{row}.termSource}",
                                                                ".csc-exhibitionAuthority-termDisplayName": "${{row}.termDisplayName}",
                                                                ".csc-exhibitionAuthority-termLanguage": {
                                                                    "decorators": [
                                                                        {
                                                                            "func": "cspace.termList",
                                                                            "type": "fluid",
                                                                            "options": {
                                                                                "root": "{row}",
                                                                                "elPath": "termLanguage",
                                                                                "termListType": "termLanguage"
                                                                            }
                                                                        }
                                                                    ]
                                                                },
                                                                ".csc-exhibitionAuthority-termStatus": {
                                                                    "default": "provisional",
                                                                    "optionnames": [
                                                                        "Provisional",
                                                                        "Under Review",
                                                                        "Accepted",
                                                                        "Rejected"
                                                                    ],
                                                                    "optionlist": [
                                                                        "provisional",
                                                                        "under review",
                                                                        "accepted",
                                                                        "rejected"
                                                                    ],
                                                                    "selection": "${{row}.termStatus}"
                                                                }
                                                            },
                                                            "type": "fluid.noexpand"
