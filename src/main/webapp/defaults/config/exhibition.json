{
    "pageBuilder": {
        "options": {
            "pageType": "exhibition",
            "components": {
                "recordEditor": {
                    "type": "cspace.recordEditor",
                    "options": {
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "fieldsToIgnore": [
                            "csid",
                            "fields.csid",
                            "fields.shortIdentifier",
                            "fields.narrowerContexts"
                        ],
                        "selectors": {
                            "identificationNumber": ".csc-exhibitionAuthority-termDisplayName"
                        }
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            {
                                "type": "repeatableMatch",
                                "queryPath": "fields.exhibitionTermGroup",
                                "childPath": "_primary",
                                "value": true,
                                "path": "termDisplayName"
                            }
                        ]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                },
                "tabs": {
                    "type": "cspace.tabs"
                },
                "sidebar": {
                    "type": "cspace.sidebar"
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "exhibition",
            "schema": {
                "exhibition": null
            }
        }
    }
}